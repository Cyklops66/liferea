              c:identifier="social_get_bookmark_site">
      <return-value transfer-ownership="none">
        <doc xml:space="preserve">the name of the currently configured social bookmarking site.</doc>
        <type name="utf8" c:type="const gchar*"/>
      </return-value>
    </function>
    <function name="social_get_bookmark_url"
              c:identifier="social_get_bookmark_url">
      <doc xml:space="preserve">Returns a social bookmarking link for the configured site</doc>
      <return-value transfer-ownership="full">
        <doc xml:space="preserve">new URL string</doc>
        <type name="utf8" c:type="gchar*"/>
      </return-value>
      <parameters>
        <parameter name="link" transfer-ownership="none">
          <doc xml:space="preserve">the link to encode (mandatory)</doc>
          <type name="utf8" c:type="const gchar*"/>
        </parameter>
        <parameter name="title" transfer-ownership="none">
          <doc xml:space="preserve">the title to encode (mandatory)</doc>
          <type name="utf8" c:type="const gchar*"/>
        </parameter>
      </parameters>
    </function>
    <function name="social_init" c:identifier="social_init" introspectable="0">
      <doc xml:space="preserve">Initialize social bookmarking support.</doc>
      <return-value transfer-ownership="none">
        <type name="none" c:type="void"/>
      </return-value>
    </function>
    <function name="social_register_bookmark_site"
              c:identifier="social_register_bookmark_site">
      <doc xml:space="preserve">Add a new site to the social bookmarking site list.</doc>
      <return-value transfer-ownership="none">
        <type name="none" c:type="void"/>
      </return-value>
      <parameters>
        <parameter name="name" transfer-ownership="none">
          <doc xml:space="preserve">descriptive name</doc>
          <type name="utf8" c:type="const gchar*"/>
        </parameter>
        <parameter name="url" transfer-ownership="none">
          <doc xml:space="preserve">valid HTTP GET URL with one or two %s format codes</doc>
          <type name="utf8" c:type="const gchar*"/>
        </parameter>
        <parameter name="title" transfer-ownership="none">
          <doc xml:space="preserve">TRUE if site accepts titles (URL must have two %s format codes!)</doc>
          <type name="gboolean" c:type="gboolean"/>
        </parameter>
        <parameter name="titleFirst" transfer-ownership="none">
          <doc xml:space="preserve">TRUE if title is first format code (title must be TRUE)</doc>
          <type name="gboolean" c:type="gboolean"/>
        </parameter>
      </parameters>
    </function>
    <function name="social_set_bookmark_site"
              c:identifier="social_set_bookmark_site">
      <doc xml:space="preserve">Change the site used for bookmarking.</doc>
      <return-value transfer-ownership="none">
        <type name="none" c:type="void"/>
      </return-value>
      <parameters>
        <parameter name="name" transfer-ownership="none">
          <doc xml:space="preserve">name of the site</doc>
          <type name="utf8" c:type="const gchar*"/>
        </parameter>
      </parameters>
    </function>
    <record name="subscriptionType" c:type="subscriptionType">
      <field name="prepare_update_request" introspectable="0">
        <callback name="prepare_update_request" introspectable="0">
          <return-value transfer-ownership="none">
            <type name="gboolean" c:type="gboolean"/>
          </return-value>
          <parameters>
            <parameter name="subscription" transfer-ownership="none">
              <type c:type="subscriptionPtr"/>
            </parameter>
            <parameter name="request" transfer-ownership="none">
              <type name="gpointer" c:type="updateRequest*"/>
            </parameter>
          </parameters>
        </callback>
      </field>
      <field name="process_update_result" introspectable="0">
        <callback name="process_update_result" introspectable="0">
          <return-value transfer-ownership="none">
            <type name="none" c:type="void"/>
          </return-value>
          <parameters>
            <parameter name="subscription" transfer-ownership="none">
              <type c:type="subscriptionPtr"/>
            </parameter>
            <parameter name="result" transfer-ownership="none">
              <type name="gpointer" c:type="const updateResult* const"/>
            </parameter>
            <parameter name="flags" transfer-ownership="none">
              <type c:type="updateFlags"/>
            </parameter>
          </parameters>
        </callback>
      </field>
    </record>
    <record name="subscriptionTypePtr"
            c:type="subscriptionTypePtr"
            disguised="1">
    </record>
    <record name="tabInfo" c:type="tabInfo">
      <field name="label" writable="1">
        <type name="Gtk.Widget" c:type="GtkWidget*"/>
      </field>
      <field name="widget" writable="1">
        <type name="Gtk.Widget" c:type="GtkWidget*"/>
      </field>
      <field name="htmlview" writable="1">
        <type name="HtmlView" c:type="LifereaHtmlView*"/>
      </field>
    </record>
  </namespace>
</repository>
